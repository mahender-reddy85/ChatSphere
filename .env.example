# Environment variables for ChatSphere
# Copy this file to .env.local and fill in your values for local development.
# Do NOT commit .env.local to version control.

# Gemini API Key for AI bot features (get from https://aistudio.google.com/app/apikey)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Base URL for asset paths (optional, defaults to '/')
# Set to '/ChatSphere/' for GitHub Pages build
# VITE_BASE_URL=/ChatSphere/
